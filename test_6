const request = require('supertest');

// https://swapi.dev/api
// /people/1...
// /films/1...
// /species/1...
// /vehicles/1...
// /starships/1...
// /planets/1...

test('Deve receber uma mensagem de erro se o clima do planta estiver incorreto', async () => {
    const answer = await request('https://swapi.dev/api').get('/planets/1681') //planets/1681 - 404 error
    
    // Verifica se o status de requisição está retornando error
    expect(answer.status).toBe(404);
});